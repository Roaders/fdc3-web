{
    "name": "sample-node",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "/projects/sample-node/src",
    "projectType": "application",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/esbuild:esbuild",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/projects/sample-node",
                "format": [
                    "cjs"
                ],
                "main": "projects/sample-node/src/main.ts",
                "tsConfig": "projects/sample-node/tsconfig.build.json"
            }
        },
        "serve": {
            "continuous": true,
            "executor": "@nx/js:node",
            "defaultConfiguration": "development",
            "dependsOn": [
                "build"
            ],
            "options": {
                "buildTarget": "sample-node:build",
                "runBuildTargetDependencies": false
            },
            "configurations": {
                "development": {
                    "buildTarget": "sample-node:build:development"
                },
                "production": {
                    "buildTarget": "sample-node:build:production"
                }
            }
        }
    }
}